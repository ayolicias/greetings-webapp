language: node_js
 env:
  - DATABASE_URL=postgresql://postgres@localhost:5432/greeted_app_database';
 
 node_js:  
  - "8.9.4"
 before_script:
  - psql -c 'create database  greeted_app_database';' -U postgres
  - psql -U postgres -d greeted_app_database -a -f users.sql;
 
addons:
    apt: 
    packages:postgresql-server 9.2
